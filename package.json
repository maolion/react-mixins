    "prepublishOnly": "rimraf dist && npm run build",
    "deploy": "mocha && npm publish",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maolion/react-mixins-decorator.git"
  },
  "keywords": [
    "react",
    "react native",
    "mixin",
    "react decorator",
    "react mixin",
    "react mixin decorator"
  ],
  "author": "maolion.j@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maolion/react-mixins-decorator/issues"
  },
  "homepage": "https://github.com/maolion/react-mixins-decorator#readme",
  "devDependencies": {
    "@types/chai": "^3.4.35",
    "@types/mocha": "^2.2.40",
    "@types/react": "^15.0.21",
    "chai": "^3.5.0",
    "mocha": "^3.2.0",
    "rimraf": "^2.6.1",
    "tslint": "^5.0.0",
    "typescript": "^2.2.2",
    "vts": "^5.0.1"
  },
  "dependencies": {
    "react": "^16.0.0-alpha.6"
  }
