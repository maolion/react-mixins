{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "declaration": false,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "removeComments": true,
    "skipDefaultLibCheck": true,
    "noEmitOnError": false,
    "preserveConstEnums": true,
    "noLib": false,
    "sourceMap": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "outDir": "./dist",
    "jsx": "react"
  },
  "exclude": [
    "node_modules",
    ".vscode",
    "dist"
  ]
}
